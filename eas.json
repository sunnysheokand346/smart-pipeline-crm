{
  "cli": {
    "version": ">= 3.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "image": "ubuntu-20.04-jdk-11-ndk-r23b"
      }
    },
    "apk_test": {
      "android": {
        "buildType": "apk",
        "image": "ubuntu-20.04-jdk-11-ndk-r23b"
      },
      "distribution": "internal",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
